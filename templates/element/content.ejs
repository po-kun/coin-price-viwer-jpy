<div class="container-fluid">
	<%# rowは３個置き %>
	
	<%# generateTable関数を定義 %>
	<% var generateTable = function(tables){ %>
		<% Object.keys(tables).forEach(function (key, index) { %>
		
		<% var i = index + 1 %>
			<% if(index == 0 || (i % 4) == 0) { %>
			<div class="row">
		<% } %>
		
		<div class="col-12 col-md-4">
			<div class="row">
				<div class="col-4"><h5><%= key %></h5></div>
				<div class="cmp col-8">MarketCap (24H) : <span class="<%= key.toLowerCase() %>_cm"></span></div>
			</div>
			<table class="table table-dark">
			<thead class="">
				<tr>
					<th width="40%">Exchange</th>
					<th width="60%">Price</th>
				</tr>
			</thead>
			<tbody>
				<% tables[key].rows.forEach(function(row,j){ %>
				<tr>
				<td><%= vars.curencyExchangers[row] %></td>
				<td id="<%= tables[key].prefix + row.toLowerCase() %>"></td>
				</tr>
				<% }) %>
			</tbody>
			</table>
		</div>
		
		<% if(index != 0 && (i % 3) == 0) { %>
			</div>
		<% } %> 
		
		<% }); %>
	</ul>
	<% }; %>
	
	<%- generateTable(vars.tables); %>
	
</div>